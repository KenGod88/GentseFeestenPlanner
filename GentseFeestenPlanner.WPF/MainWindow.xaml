<Window x:Class="GentseFeestenPlanner.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GentseFeestenPlanner.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListView Name="userListView"  Margin="10">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="User ID"  Width="100"/>
                    <GridViewColumn Header="First Name"  Width="150"/>
                    <GridViewColumn Header="Last Name" Width="150"/>
                    
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
